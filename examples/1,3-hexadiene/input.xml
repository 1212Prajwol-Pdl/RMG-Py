<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Tracks the consumption of 1,3-hexadiene in presence of methane, hydrogen, and nitrogen. -->

<rmginput>
	<!-- Data sources -->
	<database type="general">RMG_database</database>
	<!-- List of initial species -->
	<species id="spec1" label="HXD13" reactive="yes">
		<!--<cml:molecule xmlns:cml="http://www.xml-cml.org/schema">
			<cml:atomArray>
				<cml:atom id="a1" elementType="C" />
				<cml:atom id="a2" elementType="C" />
				<cml:atom id="a3" elementType="C" />
				<cml:atom id="a4" elementType="C" />
				<cml:atom id="a5" elementType="C" />
				<cml:atom id="a6" elementType="C" />
			</cml:atomArray>
			<cml:bondArray>
				<cml:bond order="D" atomRefs2="a1 a2" />
				<cml:bond order="S" atomRefs2="a2 a3" />
				<cml:bond order="D" atomRefs2="a3 a4" />
				<cml:bond order="S" atomRefs2="a4 a5" />
				<cml:bond order="S" atomRefs2="a5 a6" />
			</cml:bondArray>
		</cml:molecule>-->
		<inchi>InChI=1/C6H10/c1-3-5-6-4-2/h3,5-6H,1,4H2,2H3/b6-5+</inchi>
	</species>
	<species id="spec3" label="CH4" reactive="yes">
		<inchi>InChI=1/CH4/h1H4</inchi>
	</species>
	<species id="spec6" label="H2" reactive="yes">
		<smiles>[H][H]</smiles>
	</species>
	<!-- Inert gases -->
	<species id="spec0" label="N2" reactive="no">
		<inchi>InChI=1/N2/c1-2</inchi>
	</species>
	<!-- List of reaction systems -->
	<reactionSystem>
		<temperatureModel type="isothermal">
			<temperature units="K">1350.0</temperature>
		</temperatureModel>
		<pressureModel type="isobaric">
			<pressure units="bar">1.0</pressure>
		</pressureModel>
		<concentration speciesID="spec1" units="mol/cm**3">6.829e-4</concentration>
		<concentration speciesID="spec3" units="mol/cm**3">0.104</concentration>
		<concentration speciesID="spec6" units="mol/cm**3">0.0156</concentration>
		<concentration speciesID="spec0" units="mol/cm**3">0.8797</concentration>
	</reactionSystem>
	<!-- Termination -->
	<termination>
		<target type="conversion" speciesID="1">0.9</target>
		<target type="time" units="s">1e12</target>
	</termination>
	<!-- Dynamic simulator -->
	<simulator name="DASSL" atol="1e-18" rtol="1e-8" />
	
	<!-- Options -->
	<modelTolerance>0.1</modelTolerance>
	<inchiGeneration>off</inchiGeneration>
	<spectralDataEstimation>off</spectralDataEstimation>
	<activatedNetworks>off</activatedNetworks>
	
</rmginput>
