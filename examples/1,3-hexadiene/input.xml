<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Tracks the consumption of 1,3-hexadiene in presence of methane, hydrogen, and nitrogen. -->

<rmginput>
	<!-- The units to use for RMG output: si or cgs -->
	<units>si</units>
	<!-- Data sources -->
	<database type="general">RMG_database</database>
	<!-- List of initial species -->
	<species id="spec1" label="HXD13" reactive="yes">
		<cml>
			<molecule>
				<atomArray>
					<atom id="a1" elementType="C" />
					<atom id="a2" elementType="C" />
					<atom id="a3" elementType="C" />
					<atom id="a4" elementType="C" />
					<atom id="a5" elementType="C" />
					<atom id="a6" elementType="C" />
				</atomArray>
				<bondArray>
					<bond atomRefs2="a1 a2" order="D" />
					<bond atomRefs2="a2 a3" order="S" />
					<bond atomRefs2="a3 a4" order="D" />
					<bond atomRefs2="a4 a5" order="S" />
					<bond atomRefs2="a5 a6" order="S" />
				</bondArray>
			</molecule>
		</cml>
	</species>
	<species id="spec3" label="CH4" reactive="yes">
		<inchi>InChI=1/CH4/h1H4</inchi>
	</species>
	<species id="spec6" label="H2" reactive="yes">
		<smiles>[H][H]</smiles>
	</species>
	<!-- Inert gases -->
	<species id="spec0" label="N2" reactive="no">
		<inchi>InChI=1/N2/c1-2</inchi>
	</species>
	<!-- List of reaction systems -->
	<reactionSystem type="batch">
		<temperatureModel type="isothermal">
			<temperature units="K">1350.0</temperature>
		</temperatureModel>
		<pressureModel type="isobaric">
			<pressure units="bar">1.0</pressure>
		</pressureModel>
		<physicalPropertyModel type="idealGas" />
		<moleFraction speciesID="spec1">6.829e-4</moleFraction>
		<moleFraction speciesID="spec3">0.104</moleFraction>
		<moleFraction speciesID="spec6">0.0156</moleFraction>
		<moleFraction speciesID="spec0">0.8797</moleFraction>
	</reactionSystem>
	<!-- Termination -->
	<termination>
		<target type="conversion" speciesID="1">0.9</target>
		<target type="time" units="s">1e12</target>
	</termination>
	<!-- Dynamic simulator -->
	<simulator atol="1e-8" rtol="1e-4" />
	<!-- Options -->
	<fluxTolerance>0.1</fluxTolerance>
	<inchiGeneration>off</inchiGeneration>
	<spectralDataEstimation>off</spectralDataEstimation>
	<activatedNetworks>off</activatedNetworks>
	
</rmginput>
