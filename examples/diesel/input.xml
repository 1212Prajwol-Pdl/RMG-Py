<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Tracks the autoxidation of diesel in the liquid phase -->
<!-- pauses for a very long time after 183 species in the edge -->

<rmginput>
	<!-- The units to use for RMG output: si or cgs -->
	<units>si</units>
	<!-- Data sources -->
	<database type="general">RMG_database</database>
	<!-- List of initial species -->
	<species id="n-decylbenzene" label="n-decylbz" reactive="yes">
		<smiles>CCCCCCCCCCc1ccccc1</smiles>
	</species>
	<species id="oxygen" label="O2" reactive="yes">
		<smiles>O=O</smiles>
	</species>
	<!-- Inert gases
	<species id="spec0" label="N2" reactive="no">
		<inchi>InChI=1/N2/c1-2</inchi>
	</species> -->
	<!-- List of reaction systems -->
	<reactionSystem type="batch">
		<temperatureModel type="isothermal">
			<temperature units="K">500.0</temperature>
		</temperatureModel>
		<pressureModel type="isobaric">
			<pressure units="bar">1.0</pressure>
		</pressureModel>
		<physicalPropertyModel type="incompressibleLiquid" >
		    <molarVolume units="cm^3/mol">162.0</molarVolume>
		</physicalPropertyModel>
		<moleFraction speciesID="n-decylbenzene">0.5</moleFraction>
		<moleFraction speciesID="oxygen">0.5</moleFraction>
	</reactionSystem>
	<!-- Termination -->
	<termination>
		<target type="conversion" speciesID="oxygen">0.5</target>
		<target type="time" units="s">1e12</target>
	</termination>
	<!-- Dynamic simulator -->
	<simulator atol="1e-16" rtol="1e-8" />
	<fluxTolerance>0.1</fluxTolerance>
	<!-- Options -->
	<drawMolecules>on</drawMolecules>
	<generatePlots>on</generatePlots>
	<!--<inchiGeneration>off</inchiGeneration>
	<spectralDataEstimation>off</spectralDataEstimation>
	<activatedNetworks>off</activatedNetworks>-->
	
</rmginput>
